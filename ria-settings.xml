<settings>
    <mirrors>
        <mirror>
            <!--This sends everything else to /public -->
            <id>nexus</id>
            <mirrorOf>external:*</mirrorOf>
            <url>https://nexus.riaint.ee/repository/maven-public/</url>
        </mirror>
    </mirrors>
    <servers>
        <server>
            <id>credentials</id>
            <username>${NEXUS_user}</username>
            <password>${NEXUS_pass}</password>
        </server>
    </servers>
</settings>
